@model Product




    <h3 class="text-center py-4">Delete Product</h3>


<a asp-action="Index" asp-controller="Product" class="btn btn-primary">Back To Index</a>

<form method="post">
    <div class="mb-3">
        <label asp-for="Title" class="form-label"></label>
        <input disabled asp-for="Title" class="form-control">
    </div>
    <div class="mb-3">
        <label asp-for="ISBN" class="form-label"></label>
        <input disabled asp-for="ISBN" class="form-control">
    </div>
    <div class="mb-3">
        <label asp-for="Author" class="form-label"></label>
        <input disabled asp-for="Author" class="form-control">
    </div>
    <div class="mb-3">
        <label asp-for="ListPrice" class="form-label"></label>
        <input disabled asp-for="ListPrice" class="form-control">
    </div>
    <div class="mb-3">
        <label asp-for="Price50" class="form-label"></label>
        <input disabled asp-for="Price50" class="form-control">
    </div>
    <div class="mb-3">
        <label asp-for="Price100" class="form-label"></label>
        <input disabled asp-for="Price100" class="form-control">
    </div>
    <div class="mb-3">
        <label asp-for="Price" class="form-label"></label>
        <input disabled asp-for="Price" class="form-control">
    </div>
    <div class="mb-3">
        <label asp-for="Description" class="form-label"></label>
        <input disabled asp-for="Description" class="form-control">
    </div>
    <button type="submit" class="btn btn-danger">Delete</button>
</form>



@section Scripts{
    <scripts>
        <partial name="_ValidationScriptsPartial" />
    </scripts>
}